{
  "main": "./src/App.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "prestorybook": "rnstl --searchDir ./src/components --pattern \"**/stories.js\"",
    "storybook": "expo start --web --config ./app-storybook.json"
  },
  "engines": {
    "node": ">12.0.0"
  },
  "dependencies": {
    "expo": "^37.0.0",
    "expo-constants": "^9.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-native": "expo/react-native#sdk-37",
    "react-native-web": "^0.11.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-router-native": "^5.0.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addons": "^5.2.0",
    "@storybook/react-native": "^5.2.0",
    "babel-preset-expo": "^8.0.0",
    "expo-cli": "^3.21.3",
    "jest-expo": "^37.0.0",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "^16.9.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-router|@unimodules/.*|sentry-expo|native-base|apollo-boost)"
    ]
  }
}
